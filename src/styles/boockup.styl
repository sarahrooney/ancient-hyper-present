.container-bk {
  width: 100%;
  height 761px
  background-color: rgba(11, 255, 0, 1);
  overflow: hidden;
  -webkit-perspective: 1200px;
  perspective: 1200px;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}
.container-bk > div {
  -webkit-transform-style: preserve-3d;
  transform-style: preserve-3d;
}
.boockup {
  -webkit-transform: rotateX(-1deg) rotateY(150deg) rotateZ(0deg);
  transform: rotateX(-1deg) rotateY(150deg) rotateZ(0deg);
}
.book-container {
  -webkit-transform-style: preserve-3d;
  transform-style: preserve-3d;
  -webkit-animation: 10s rotatingAnimationY linear infinite;
  animation: 10s rotatingAnimationY linear infinite;
}
.book-container > div {
  position: absolute;
  top: 0;
  left: 0;
  background-size: cover;
  background-position: center center;
  -webkit-backface-visibility: visible;
  backface-visibility: visible;
}
.book-top, .book-bottom, .book-side-right {
  background-size: auto auto;
  background-repeat: repeat;
  background-position: top left;
}
.book-front {
  background-image: url('./../assets/images/book/book-cover.jpg');
  width: 386px;
  height: 540px;
  -webkit-transform: translate3d(-193px ,-270px ,15px) translate3d(0,0,0) rotateY(0deg);
  transform: translate3d(-193px ,-270px ,15px) translate3d(0,0,0) rotateY(0deg);
}
.book-side-left {
  background-image: url('./../assets/images/book/book-spine.jpg');
  width: 30px;
  height: 540px;
  -webkit-transform: translate3d(-193px ,-270px ,15px) translate3d(-15px, 0, -15px) rotateY(-90deg);
  transform: translate3d(-193px ,-270px ,15px) translate3d(-15px, 0, -15px) rotateY(-90deg);
}
.book-side-right {
  background-image: url('./../assets/images/book/book-side.jpg');
  width: 30px;
  height: 540px;
  -webkit-transform: translate3d(-193px ,-270px ,15px) translate3d(371px, 0px,-15px) rotateY(90deg);
  transform: translate3d(-193px ,-270px ,15px) translate3d(371px, 0px,-15px) rotateY(90deg);
}
.book-top {
  background-image: url('./../assets/images/book/book-top.jpg');
  background-position: bottom right;
  width: 386px;
  height: 30px;
  -webkit-transform: translate3d(-193px, -270px ,15px) translate3d(0px, -15px, -15px) rotateX(90deg);
  transform: translate3d(-193px, -270px ,15px) translate3d(0px, -15px, -15px) rotateX(90deg);
}
.book-bottom {
  background-image: url('./../assets/images/book/book-top.jpg');
  width: 386px;
  height: 30px;
  -webkit-transform: translate3d(-193px ,-270px ,15px) translate3d(0px, 525px, -15px) rotateX(-90deg);
  transform: translate3d(-193px ,-270px ,15px) translate3d(0px, 525px, -15px) rotateX(-90deg);
}
.book-back {
  background-image: url('./../assets/images/book/book-back.jpg');
  width: 386px;
  height: 540px;
  -webkit-transform: translate3d(-193px ,-270px ,15px) translate3d(0,0, -30px) rotateY(180deg);
  transform: translate3d(-193px ,-270px ,15px) translate3d(0,0, -30px) rotateY(180deg);
}
#credit {
  position: fixed;
  bottom: 1em;
  right: 1em;
  z-index: 99;
  background-color: #fff;
  border-radius: 8px;
  box-sizing: border-box;
  padding: .3rem 1rem;
  width: 21rem;
  height: 20px;
  -webkit-transition: background .2s;
  transition: background .2s;
}
#credit:hover {
  background-color: #d8d8d8;
}
      